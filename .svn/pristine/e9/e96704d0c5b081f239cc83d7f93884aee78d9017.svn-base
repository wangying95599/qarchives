<script type="text/javascript">
    function datetimepickerinit($table) {
        $table.datetimepicker({
            language: 'zh-CN',
            format: 'yyyy-mm-dd',
            minView: 'month',
            autoclose: true,
            todayBtn: true,
            pickerPosition: "bottom-left"
        });
    }

    $(function () {
        datetimepickerinit($('#datetimepicker'));
        datetimepickerinit($('#datetimepicker1'));
        datetimepickerinit($('#datetimepicker2'));
        datetimepickerinit($('#datetimepicker3'));
        datetimepickerinit($('#datetimepicker4'));
        datetimepickerinit($('#datetimepicker5'));
        datetimepickerinit($('#datetimepicker6'));
        datetimepickerinit($('#datetimepicker7'));
        datetimepickerinit($('#datetimepicker8'));

    });
</script>

